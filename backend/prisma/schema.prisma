datasource db {
  provider = "postgresql"
  url=env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum SocialProviderType {
  PERSONAL
  SOCIAL_MEDIA
}

model SocialProvider {
  id  String @id @default(cuid())
  name  String @unique
  baseUrl String
  providerType SocialProviderType
}

model FoodTruck {
  id   String @id @default(cuid())
  name String
  imagePath String?
  foodGenreId String
  acceptsDiningDollars Boolean
  menu Json @db.Json
}